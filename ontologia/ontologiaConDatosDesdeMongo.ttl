@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdn: <http://www.semanticweb.org/sistemaDesnutricion/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.semanticweb.org/sistemaDesnutricion> a owl:Ontology .

sdn:perteneceAIntervaloEdad a owl:ObjectProperty ;
    rdfs:domain sdn:Ninio ;
    rdfs:range sdn:IntervaloEdad .

sdn:resideEnParroquia a owl:ObjectProperty ;
    rdfs:domain sdn:Ninio ;
    rdfs:range sdn:Parroquia .

sdn:resideEnProvincia a owl:ObjectProperty ;
    rdfs:domain sdn:Ninio ;
    rdfs:range sdn:Provincia .

sdn:tieneDescripcionNivel a owl:DatatypeProperty ;
    rdfs:domain sdn:NivelDesnutricion ;
    rdfs:range xsd:string .

sdn:tieneEdadEnMeses a owl:DatatypeProperty ;
    rdfs:domain sdn:Ninio ;
    rdfs:range xsd:integer .

sdn:tieneFechaNacimiento a owl:DatatypeProperty ;
    rdfs:domain sdn:Ninio ;
    rdfs:range xsd:dateTime .

sdn:tieneID a owl:DatatypeProperty ;
    rdfs:domain sdn:Ninio ;
    rdfs:range xsd:string .

sdn:tieneIMC a owl:DatatypeProperty ;
    rdfs:domain sdn:Ninio ;
    rdfs:range xsd:float .

sdn:tieneNivelDesnutricion a owl:ObjectProperty ;
    rdfs:domain sdn:Ninio ;
    rdfs:range sdn:NivelDesnutricion .

sdn:tieneNombreParroquia a owl:DatatypeProperty ;
    rdfs:domain sdn:Parroquia ;
    rdfs:range xsd:string .

sdn:tieneNombreProvincia a owl:DatatypeProperty ;
    rdfs:domain sdn:Provincia ;
    rdfs:range xsd:string .

sdn:tienePeso a owl:DatatypeProperty ;
    rdfs:domain sdn:Ninio ;
    rdfs:range xsd:float .

sdn:tieneRangoEdad a owl:DatatypeProperty ;
    rdfs:domain sdn:IntervaloEdad ;
    rdfs:range xsd:string .

sdn:tieneSexo a owl:DatatypeProperty ;
    rdfs:domain sdn:Ninio ;
    rdfs:range xsd:string .

sdn:tieneTalla a owl:DatatypeProperty ;
    rdfs:domain sdn:Ninio ;
    rdfs:range xsd:float .

sdn:IntervaloEdad a owl:Class .

sdn:NivelDesnutricion a owl:Class .

sdn:Parroquia a owl:Class .

sdn:Provincia a owl:Class .

sdn:Ninio a owl:Class .

