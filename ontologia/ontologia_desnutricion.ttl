
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix fhir: <http://hl7.org/fhir/> .
@prefix sct:  <http://snomed.info/id/> .
@prefix geo:  <http://www.opengis.net/ont/geosparql#> .
@prefix qb:   <http://purl.org/linked-data/cube#> .
@prefix ex:   <http://example.org/tesis/> .

ex:Nino123 a fhir:Patient ;
    fhir:birthDate "2022-04-15"^^xsd:date ;
    fhir:gender "male" ;
    fhir:condition sct:422587007 ; # Código SNOMED de desnutrición crónica
    ex:resideEn ex:ParroquiaLoja ;
    ex:nivelDesnutricion ex:Moderado ;
    fhir:bodyWeight "6.8"^^xsd:decimal .

ex:Moderado a skos:Concept ;
    skos:prefLabel "Desnutrición moderada"@es ;
    skos:broader ex:Desnutricion .

ex:ParroquiaLoja a geo:Feature ;
    geo:hasGeometry ex:GeomLoja ;
    rdfs:label "Parroquia San Sebastián"@es .
